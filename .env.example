# Environment Variables
# Copy to .env and fill in your values

# Server
SERVER_PORT=8080
SERVER_ENV=development
DEBUG=true

# WeChat Configuration (REQUIRED)
WECHAT_APPID=your_appid_here
WECHAT_APPSECRET=your_appsecret_here
WECHAT_TOKEN=your_token_here
WECHAT_ENCODING_AES_KEY=your_aes_key_here

# Redis
REDIS_ADDR=localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=
DB_NAME=wechat_service
